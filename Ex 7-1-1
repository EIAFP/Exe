#include <iostream>
using namespace std;
#define N 3
#define N1 9
#define N2 9
#define N3 9
int main()
{
    int a,b,i,j,x,y, z[N]={1,2,3};
    for(a=1;a<=9;a++){
        printf("\n");
        for(b=1;b<=3;b++){
           printf("%2d*%2d=%2d\t",b,a,b*a); 
        }
    }
    printf("\n");
    for(i=1;i<=9;i++){
        printf("\n");
        for(j=4;j<=6;j++){
           printf("%2d*%2d=%2d\t",j,i,j*i); 
        }
    }
    printf("\n");
    for(x=1;x<=9;x++){
        printf("\n");
        for(y=7;y<=9;y++){
           printf("%2d*%2d=%2d\t",y,x,y*x); 
        }
    }
     return 0;
}
