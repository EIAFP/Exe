#include <iostream>
using namespace std;
int main()
{
    int a,b,c,d,e,f,g,h,i,j,q=0,w=0,r=0,L, x[10]={a,b,c,d,e,f,g,h,i,j};
    for(L=1;L<=10;L++){
        scanf("%d",&x[L]);
        if (x[L]>0)
            q=q+1;
        else
            if (x[L]<0)
                w=w+1;
            else
                r=r+1;
    }   
    printf("正數有%d個\n零有%d個\n負數有%d個\n",q,r,w);
}
