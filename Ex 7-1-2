#include <iostream>
using namespace std;
#define N 3
int main()
{
    int a,b,c, x[N]={a,b,c};
    scanf("%d%d%d",&a,&b,&c);
    if (a>b)
        if (b>c)
            printf("%d>%d>%d",a,b,c);
        else
            printf("%d>%d>%d",a,c,b);
    else
        if (a>c)
            printf("%d>%d>%d",b,a,c);
        else
            if (b>c)
                printf("%d>%d>%d",b,c,a);
            else
                if (c>b)
                    printf("%d>%d>%d",c,b,a);
                else
                    printf("%d>%d>%d",c,a,b);
    return 0;
}
